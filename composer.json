{
    "name": "tipoff/discounts",
    "description": "Laravel Package for Discount and Promo Codes",
    "keywords": [
        "tipoff",
        "discounts"
    ],
    "homepage": "https://github.com/tipoff/discounts",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "tipoff/authorization": "^2.7.1",
        "tipoff/checkout": "^2.6.0",
        "tipoff/support": "^1.8.9"
    },
    "require-dev": {
        "tipoff/test-support": "^1.4.2"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\Discounts\\": "src",
            "Tipoff\\Discounts\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Discounts\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Discounts\\DiscountsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
